{
  "name": "laravel/lumen",
  "description": "The Laravel Lumen Framework.",
  "keywords": [
    "framework",
    "laravel",
    "lumen"
  ],
  "license": "MIT",
  "type": "project",
  "repositories": [
    {
        "url": "https://github.com/GregorVoelkl/laravel-paypal.git",
        "type": "git"
    }
  ],
  "require": {
    "php": ">=7.4.0",
    "laravel/lumen-framework": "^8.0",
    "laravel/helpers": "^1.6",
    "laravel/legacy-factories": "^1.3",
    "vlucas/phpdotenv": "^5.2",
    "league/flysystem": "~2.5",
    "php-open-source-saver/jwt-auth": "^1.1",
    "lcobucci/jwt": "^4.0.0",
    "illuminate/routing": "^8.0",
    "illuminate/hashing": "^8.0",
    "illuminate/cookie": "^8.0",
    "dingo/api": "^3.0",
    "barryvdh/laravel-cors": "^2.2",
    "doctrine/dbal": "^3.3",
    "illuminate/mail": "^8.0",
    "guzzlehttp/guzzle": "~5.3|~6.0",
    "ext-fileinfo": "*",
    "intervention/image": "^2.7",
    "nesbot/carbon": "^2.0",
    "google/recaptcha": "~1.1",
    "florianv/swap": "4.3.*",
    "guzzlehttp/oauth-subscriber": "0.6.0",
    "barryvdh/laravel-ide-helper": "^2.6",
    "tedivm/jshrink": "~1.5",
    "easy-slug/easy-slug": "2.0.*",
    "paypal/rest-api-sdk-php": "^1.13",
    "netshell/paypal": "dev-master",
    "php-http/guzzle6-adapter": "^2.0"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "phpunit/phpunit": "~7.0",
    "filp/whoops": "~2.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/",
      "Plugins\\": "app/Plugins",
      "Intervention\\Image\\": "src/Intervention/Image"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/",
      "database/"
    ],
    "files": [
      "app/helpers.php"
    ]
  },
  "config": {
      "preferred-install": "dist",
      "sort-packages": true,
      "optimize-autoloader": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
